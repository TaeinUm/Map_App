    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "immutable": "^5.0.0-beta.4",
    "mapbox-gl": "^2.15.0",
    "mapbox-gl-inspect": "^1.3.1",
    "react": "^18.2.0",
